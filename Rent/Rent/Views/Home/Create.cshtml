

@model Rent.Models.Rents

<div class="container">

    <form  method="post" enctype="multipart/form-data">
        @Html.AntiForgeryToken()
        <div class="form-group">
            @Html.LabelFor(m => m.Model)
            @Html.TextBoxFor(m => m.Model, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Marka)
            @Html.TextBoxFor(m => m.Marka, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Price)
            @Html.TextBoxFor(m => m.Price, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Desc)
            @Html.TextBoxFor(m => m.Desc, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Photo)
            @Html.TextBoxFor(m => m.Photo, new { @class = "form-control", type = "file" })
        </div>

        <input type="submit" class="btn btn-success" value="Create Book" />

    </form>

</div>
